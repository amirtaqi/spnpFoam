FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0/solid";
    object      r;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    lowerWall
    {
        type            symmetry;
    }
    upperWall
    {
        type            symmetry;
    }
    emptyWall
    {
        type            empty;
    }
    solid_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
}


